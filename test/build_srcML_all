#!/bin/bash

./prlctlall.py --fedora --build resume; sleep 5
./build_srcML fedora
./prlctlall.py --fedora --build suspend

./prlctlall.py --ubuntu --build resume; sleep 5
./build_srcML ubuntu
./prlctlall.py --ubuntu --build suspend

./prlctlall.py --mint --build resume; sleep 5
./build_srcML mint
./prlctlall.py --mint --build suspend

./prlctlall.py --suse --build resume; sleep 5
./build_srcML suse
./prlctlall.py --suse --build suspend

./prlctlall.py --mac --build resume; sleep 5
./build_srcML mac
./prlctlall.py --mac --build suspend